{
  "extends": ["ultracite/core", "ultracite/react", "ultracite/next"],
  "files": {
    "includes": ["**", "!components/ui"]
  },
  "linter": {
    "rules": {
      "nursery": {
        "noEmptySource": "off"
      },
      "performance": {
        "noBarrelFile": "off",
        "noImgElement": "off"
      },
      "correctness": {
        "useImageSize": "off"
      },
      "suspicious": {
        "noConsole": "off"
      }
    }
  }
}
